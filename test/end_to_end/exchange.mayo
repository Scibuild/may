import Std from "lib/std.may";

class A { 
  public id: int; 
  constructor(id: int) { this.id = id; } 
}

fun main(): unit {
  let a_array: []mut !A = [ new A(1) ];
  let a: !A = new A(2);
  Std.Int.printInfo("a.id", a.id);
  Std.Int.printInfo("a_array[0].id", a_array[0].id); 
  Std.String.println("Exchanging ....");
  a >=< a_array[0];
  Std.Int.printInfo("a.id", a.id);
  Std.Int.printInfo("a_array[0].id", a_array[0].id); 
}
